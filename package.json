{
  "name": "mittos-do-cabula",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:3001": "next dev -p 3001",
    "dev:3002": "next dev -p 3002",
    "dev:auto": "next dev -p 0",
    "build": "next build",
    "build:netlify": "cross-env BUILD_EXPORT=true next build",
    "build:vercel": "cross-env BUILD_STANDALONE=false next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "start:3001": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out",
    "python:install": "cd python-server && pip install -r requirements.txt",
    "python:dev": "cd python-server && python app.py",
    "dev:full": "concurrently \"npm run python:dev\" \"npm run dev\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "deploy:netlify": "npm run build:netlify && netlify deploy --prod --dir=out",
    "deploy:vercel": "npm run build:vercel && vercel --prod",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.441.0",
    "next": "14.2.13",
    "next-auth": "^4.24.11",
    "prisma": "^5.22.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^2.5.2",
    "tsx": "^4.20.6",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "postcss": "^8",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
